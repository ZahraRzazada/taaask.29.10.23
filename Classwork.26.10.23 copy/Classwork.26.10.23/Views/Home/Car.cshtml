@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "LayoutModel";
}
@using Classwork._26._10._23.Models;
@model List<Car>

<div>
    <ul>
        @foreach (var car in Model)
        {
            <li>
                <a asp-controller="home" asp-action="Detail" asp-route-id="@car.Id">
                    @car.Name
                </a>
            </li>

        }

    </ul>
</div>

