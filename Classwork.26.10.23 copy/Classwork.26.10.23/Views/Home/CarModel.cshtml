@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "LayoutModel";
}
@using Classwork._26._10._23.Models;
@model List<CarModel>

<div>
    <ul>
        @foreach (var carmodel in Model)
        {
            <li>
                <a asp-controller="home" asp-action="Car" asp-route-id="@carmodel.Id">
                    @carmodel.Name
                </a>
            </li>

        }

    </ul>
</div>

